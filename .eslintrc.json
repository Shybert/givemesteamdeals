{
    "extends": "airbnb-base",
    "rules": {
        "quotes": ["error", "double"],
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "no-console": ["off"],
        "no-use-before-define": ["error", {"functions": false}],
        "prefer-destructuring": ["off"],
        "consistent-return": ["off"],
        "no-await-in-loop": ["off"]
    },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    }
}